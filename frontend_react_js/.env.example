# Base URL for backend API (e.g., http://localhost:3001)
REACT_APP_API_BASE=http://localhost:3001

# Optional healthcheck path relative to API base (e.g., /health or /)
REACT_APP_HEALTHCHECK_PATH=/

# Optional JSON feature flags object string, e.g. {"charts": true}
# If not set, defaults will be used in code.
REACT_APP_FEATURE_FLAGS={"charts": true}
